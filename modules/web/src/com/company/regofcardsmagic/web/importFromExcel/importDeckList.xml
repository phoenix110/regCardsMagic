<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.company.regofcardsmagic.web.importFromExcel.Importdecklist"
        messagesPack="com.company.regofcardsmagic.web.importFromExcel">
    <dialogMode height="600"
                width="800"/>
    <layout>
        <grid id="mainLayout"
              height="100%"
              spacing="true"
              width="100%">
            <columns count="2"/>
            <rows>
                <row>
                    <textArea id="decklist"
                              height="100%"
                              width="100%"/>
                    <textArea id="guide"
                              height="100%"
                              width="100%"/>
                </row>
                <row>
                    <vbox spacing="true">
                        <hbox id="buttons"
                              spacing="true"
                              width="100%">
                            <button id="check"
                                    caption="check"/>
                            <button id="start"
                                    caption="start"/>
                        </hbox>
                        <vbox id="labels"
                              height="100%"
                              spacing="true"/>
                    </vbox>
                </row>
            </rows>
        </grid>
    </layout>
</window>
