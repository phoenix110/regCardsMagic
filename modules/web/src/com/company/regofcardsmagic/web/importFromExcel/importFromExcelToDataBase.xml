<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.company.regofcardsmagic.web.importFromExcel.Importfromexceltodatabase"
        messagesPack="com.company.regofcardsmagic.web.importFromExcel">
    <dialogMode height="600"
                width="800"/>
    <layout>
        <hbox spacing="true">
            <vbox spacing="true">
                <textField id="rowWhereFind"
                           caption="Cтрока на которой находятся заголовки"/>
                <textField id="rowMax"
                           caption="Максимальное количество строк"/>
                <textField id="cellMax"
                           caption="Максимальное количество колонок"/>
                <textField id="SheetInWorkBook"
                           caption="Номер листа в книге"/>
                <upload id="upload"/>
                <button id="button"
                        caption="Start"/>
            </vbox>
            <vbox id="generatorParams"
                  spacing="true"/>
        </hbox>
    </layout>
</window>
